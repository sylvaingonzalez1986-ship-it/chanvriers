{
  "expo": {
    "owner": "les-champs-bretons",
    "name": "Chanvriers",
    "slug": "chanvriers",
    "scheme": "chanvriers",
    "version": "1.0.2",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.sylvain.x35.chanvriers"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.8.22"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
